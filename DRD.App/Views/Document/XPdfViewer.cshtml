    <script type="text/javascript">


        var myApp = angular.module('drdApp', ['ui.bootstrap'])


        myApp.controller("drdController", function ($scope, $location, $http, $filter) {


            angular.element(document).ready(function () {
                $('[data-popup="tooltip"]').tooltip();

                setTimeout(function () {
                    $scope.initData();
                },500);

                $("#viewerContainer").css('overflow', "auto");
                $('html').css('overflow', "hidden");
                $('body').css('overflow', "hidden");
                //$('#drdController').css('overflow', "hidden");
            });

            initValues();

            function initValues() {
                $scope.document = @Html.Raw(Json.Encode(Model.obj));
                $scope.type = @Html.Raw(Json.Encode(Model.dataId));
                $scope.user = @Html.Raw(Json.Encode(Model.user));
            }

            $scope.initData = function () {
                $http.post('/document/openfile', { fileName: $scope.document.FileName }).then(function (response) {
                    if (response.data) {
                        $scope.openDocument(response.data);
                    }
                }, function (response) {
                    //error handle\
                    var x = 0;
                });
            }

            $scope.openDocument = function (fileName){
                if ($scope.document.DocumentAnnotates.length>0){
                    $scope.annos=[];
                    for(i=0;i<$scope.document.DocumentAnnotates.length;i++){
                        var ix=$scope.document.DocumentAnnotates[i];
                        var item={};
                        item.SvgId='svg'+i;
                        item.Page=ix.Page;
                        item.AnnotateType=ix.AnnotateTypeId;
                        item.LeftPosition=ix.LeftPosition;
                        item.TopPosition=ix.TopPosition;
                        item.WidthPosition=ix.WidthPosition;
                        item.HeightPosition=ix.HeightPosition;
                        item.Color=ix.Color;
                        item.BackColor=ix.BackColor;
                        item.Data=ix.Data;
                        item.Data2=ix.Data2;
                        item.Rotation=ix.Rotation;
                        item.ScaleX=ix.ScaleX;
                        item.ScaleY=ix.ScaleY;
                        item.TransitionX=ix.TransitionX;
                        item.TransitionY=ix.TransitionY;
                        item.StrokeWidth=ix.StrokeWidth;
                        item.Opacity=ix.Opacity;
                        item.Flag=ix.Flag;
                        item.FlagCode=ix.FlagCode;
                        item.FlagDate = convertJsonDate(ix.FlagDate);
                        item.FlagImage = ix.FlagImage;
                        item.CreatorId=ix.CreatorId;
                        item.ElementId=ix.ElementId;
                        item.Annotate=ix.Annotate;
                        $scope.annos.push(item);
                    }
                    angular.element("#xpdfController").scope().setAnnoItems($scope.annos);
                }
                angular.element("#xpdfController").scope().setDefaultPdf(fileName);//$scope.document.FileName);
                angular.element("#xpdfController").scope().setAnnoToolbarVisible(($scope.type==1));

                var mem=$scope.document.DocumentMembers.filter((subject) => subject.MemberId === $scope.user.Id )
                //if ($scope.validAction(mem[0].FlagPermission,4,8,16)){
                //    angular.element("#xpdfController").scope().showDownload();
                //    angular.element("#xpdfController").scope().setDownloadFileName($scope.document.FileNameOri);
                //}
                var flag=mem[0].FlagPermission;
                angular.element("#xpdfController").scope().showDownload((flag & 16)==16);
                angular.element("#xpdfController").scope().showPrint((flag & 8)==8);
                angular.element("#xpdfController").scope().setDownloadFileName($scope.document.FileNameOri);

                angular.element("#xpdfController").scope().clickObjectButtonPointer();
            }


            $scope.saveAnnos = function (creatorId) {
                var items=angular.element("#xpdfController").scope().getAnnoItems();
                var annos=[];
                for(x=0;x<items.length;x++){
                    var ix=items[x];
                    if (ix.IsDeleted) continue;
                    var item={};
                    item.Page=ix.Page;
                    item.AnnotateTypeId=(ix.AnnotateTypeId==undefined?ix.AnnotateType:ix.AnnotateTypeId);
                    item.LeftPosition=ix.LeftPosition;
                    item.TopPosition=ix.TopPosition;
                    item.WidthPosition=ix.WidthPosition;
                    item.HeightPosition=ix.HeightPosition;
                    item.Color=ix.Color;
                    item.BackColor=ix.BackColor;
                    item.Data=ix.Data;
                    item.Data2=ix.Data2;
                    item.Rotation=ix.Rotation;
                    item.ScaleX=ix.ScaleX;
                    item.ScaleY=ix.ScaleY;
                    item.TransitionX=ix.TransitionX;
                    item.TransitionY=ix.TransitionY;
                    item.StrokeWidth=ix.StrokeWidth;
                    item.Opacity=ix.Opacity;
                    item.Flag=ix.Flag;
                    item.FlagCode=ix.FlagCode;
                    item.FlagDate=ix.FlagDate;
                    item.FlagImage=ix.FlagImage;
                    item.CreatorId=ix.CreatorId;
                    item.ElementId=ix.ElementId;
                    annos.push(item);
                }
                $http.post('/document/saveannos', { documentId:$scope.document.Id, creatorId:creatorId, annos:annos}).then(function (response) {
                    if (response.data) {
                        //showInfo("Save annotations successfully");
                        Android.doAndroidSaved();
                    }
                }, function (response) {
                    //error handle\
                    var x = 0;
                });
            }

            $scope.validAction = function (val, ...bits) {
                var ret=false;
                for(i=0;i<bits.length;i++){
                    if ((val & bits[i])==bits[i]){
                        ret=true;
                        break;
                    }
                }
                return ret;
            }
            function convertJsonDate(val) {
                if (val == undefined)
                    return '';
                return new Date(parseInt(val.substr(6)));
            }

        });


        $(function () {
        });

        $(document).ready(function () {
            setFromAndroid();
        })

        function saveAnnos(creatorId){
            var scope = angular.element(document.getElementById("drdController")).scope();
            scope.$apply(function () {
                scope.saveAnnos(creatorId);
