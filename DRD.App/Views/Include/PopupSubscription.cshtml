
@{
    Layout = null;
}

<div id="modal_select_subscription" class="modal fade" z-index="10000">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header bg-primary">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h6 class="modal-title">Select Subscription</h6>
            </div>

            <div style="height:450px" class="table-responsive">
                @*class="modal-body"*@
                <table class="table-xxs table-bordered table-hover datatable-highlight dataTable no-footer" style="font-size:small;width:100%">
                    <tbody>
                        <tr data-dismiss="modal" style="cursor:pointer" ng-repeat="x in SubscriptionList" style="vertical-align:top">
                            <td width="25px" ng-click="do_start($index);">{{x.SubscriptionName}} Subscription - {{x.CompanyName}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

