
@{
    Layout = null;
}

<div id="modal_tnc" class="modal fade">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button class="close" type="button" data-dismiss="modal">×</button>
                <h5 class="modal-title">Terms and Conditions</h5>
            </div>
            <div class="modal-body">
                <iframe src="/doc/tnc/tnc.htm" frameborder="0" scrolling="auto" style="width: 100%; height:400px; background-color: white;" allowTransparency="true" allowfullscreen align="center"></iframe>
            </div>
            <div class="modal-footer">
                <button class="btn btn-link" type="button" data-dismiss="modal">Close</button>
                <button class="btn btn-primary" type="button" ng-click="goRegistry()">Agree</button>
            </div>
        </div>
    </div>
</div>

